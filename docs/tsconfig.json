{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["DOM", "ES2020"],
    "module": "ESNext",
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "moduleResolution": "bundler",
    "useDefineForClassFields": true,
    "allowImportingTsExtensions": true,
    "jsx": "react-jsx",
    "outDir": "./dist",
    "paths": {
      "@theme": ["./theme"],
      "@components/*": ["./theme/components/*"],
      "@en/*": ["./docs/en/*"],
      "i18n": ["./i18n.json"]
    },
    "baseUrl": "./"
  },
  "mdx": {
    "checkMdx": true
  },
  "include": ["docs", "rspress.config.ts", "theme", "env.d.ts"]
}
